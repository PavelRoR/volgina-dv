#video
	padding 90px 0
.video-wrapper-video
	max-width 650px
.video-title
	display inline-block
	font-size: 30px
	font-weight: bold
	color: #D0B899
	position relative
	margin-bottom: 60px
	&:before
		content: ''
		display block
		position absolute
		bottom -10px
		right -20%
		width 269px
		height: 5px
		background-color: #B2DFBE
